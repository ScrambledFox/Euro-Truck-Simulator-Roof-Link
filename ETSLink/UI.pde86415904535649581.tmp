class UI {

  public void Tick(){
    this.Draw();
  }
  
  public void Draw(){
    pushStyle();
    
    DrawSideLine();
    
    popStyle();
  }
  
  void DrawSideLine(){
    noStroke();
    fill(255);
    rect(inalfaRoof.effectWidth * 3f, 0, 10, height);
  }
  
}
